<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="TSQMBenchmarkPluginBuilder"
         name="TSQM Benchmark Plugin Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="com.tsqm.plugin.builder.TSQMBenchmarkPluginBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="TSQMBenchmarkNatureID"
         name="TSQMBenchmarkNaturePN"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.tsqm.plugin.builder.TSQMBenchmarkNatureCN">
         </run>
      </runtime>
      <builder
            id="com.tsqm.plugin.TSQMBenchmarkPluginBuilder">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IProject"
            nameFilter="*"
            id="com.tsqm.plugin.contribution1">
         <action
               label="Add/Remove Sample Nature"
               class="com.tsqm.plugin.builder.ToggleNatureAction"
               menubarPath="additions"
               enablesFor="+"
               id="com.tsqm.plugin.addRemoveNatureAction">
         </action>
      </objectContribution>
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="com.tsqm.plugin.contribution1">
         <menu
               label="TSQM New Submenu"
               path="additions"
               id="com.tsqm.plugin.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="TSQM New Action"
               class="com.tsqm.plugin.popup.actions.TSQMNewAction"
               menubarPath="com.tsqm.plugin.menu1/group1"
               enablesFor="1"
               id="com.tsqm.plugin.newAction">
         </action>
      </objectContribution>
   </extension>
   <extension
         id="xmlProblem"
         name="XML Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="TSQM Benchmark Category"
            id="com.tsqm.plugin">
      </category>
      <view
            name="TSQM Benchmark Navigator"
            icon="icons/sample.gif"
            class="org.eclipse.ui.navigator.CommonNavigator"
            id="com.tsqm.plugin.navigator">
      </view>
      <view
            name="TSQM Benchmark Main View"
            icon="icons/sample.gif"
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.views.TSQMMainView"
            id="com.tsqm.plugin.views.TSQMMainView">
      </view>
      <view
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.views.TSQMBenchmarkViewpart"
            id="com.tsqm.plugin.views.TSQMViewpartSWT"
            name="TSQM Benchmark View">
      </view>
      <view
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.views.TSQMSCAUViewpart"
            id="com.tsqm.plugin.views.TSQMSCAUViewpart"
            name="TSQM Source Code Analysis Unit">
      </view>
      <view
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.views.WordView"
            icon="icons/sample.gif"
            id="com.tsqm.plugin.views.WordView"
            name="WordView">
      </view>
      <view
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.views.ListenerView"
            icon="icons/sample.gif"
            id="com.tsqm.plugin.views.ListenerView"
            name="ListenerView">
      </view>
      <view
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.views.TSQMMetricsView"
            icon="icons/sample.gif"
            id="com.tsqm.plugin.views.TSQMMetricsView"
            name="TSQM Metrics View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="com.tsqm.plugin.navigator">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="com.tsqm.plugin.navigator">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.navigator.ProjectExplorer"
               relationship="stack"
               id="com.tsqm.plugin.navigator">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="com.tsqm.plugin.views.TSQMMainView">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="com.tsqm.plugin.views.SampleView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="com.tsqm.plugin.views.SampleView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="com.tsqm.plugin.views.TSQMMetricsView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            lightweight="true"
            location="BOTTOM_RIGHT"
            adaptable="true"
            label="Resource Decorator"
            icon="icons/sample_decorator.gif"
            state="true"
            id="com.tsqm.plugin.decorator">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IResource">
               </objectClass>
               <or>
                  <objectClass
                        name="org.eclipse.core.resources.IProject">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
               </or>
            </and>
         </enablement>
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="TSQM XML Editor"
            extensions="xml"
            icon="icons/sample.gif"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="com.tsqm.plugin.editors.XMLEditor"
            id="com.tsqm.plugin.editors.XMLEditor">
      </editor>
      <editor
            name="TSQM Benchmark Multi-page Editor"
            extensions="mpe"
            icon="icons/sample.gif"
            contributorClass="com.tsqm.plugin.editors.TSQMMultiPageEditorContributor"
            class="com.tsqm.plugin.editors.TSQMMultiPageEditor"
            id="com.tsqm.plugin.editors.TSQMMultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="TSQMCommandCategory"
            id="com.tsqm.plugin.commands.category">
      </category>
      <command
            name="TSQMCommand 1 (MAIN)"
            categoryId="com.tsqm.plugin.commands.category"
            id="com.tsqm.plugin.commands.sampleCommand">
      </command>
      <command
            categoryId="com.tsqm.plugin.commands.category"
            id="com.tsqm.plugin.commands.sampleCommand2"
            name="TSQMCommand 2 (SCAU)">
      </command>
      <command
            categoryId="com.tsqm.plugin.commands.category"
            id="com.tsqm.plugin.commands.sampleCommand3"
            name="TSQMCommand 3 (Metrics)">
      </command>
      <command
            categoryId="com.tsqm.plugin.commands.category"
            id="com.tsqm.plugin.commands.sampleCommand4"
            name="TSQMCommand 4 (Other)">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="com.tsqm.plugin.commands.sampleCommand"
            class="com.tsqm.plugin.handlers.TSQMHandler">
      </handler>
      <handler
            class="com.tsqm.plugin.handlers.TSQMHandler"
            commandId="com.tsqm.plugin.commands.sampleCommand2">
      </handler>
      <handler
            class="com.tsqm.plugin.handlers.TSQMHandler"
            commandId="com.tsqm.plugin.commands.sampleCommand3">
      </handler>
      <handler
            class="com.tsqm.plugin.handlers.TSQMHandler"
            commandId="com.tsqm.plugin.commands.sampleCommand4">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.tsqm.plugin.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="TSQM Benchmark Menu"
               mnemonic="M"
               id="com.tsqm.plugin.menus.sampleMenu">
            <command
                  commandId="com.tsqm.plugin.commands.sampleCommand"
                  id="com.tsqm.plugin.menus.sampleCommand"
                  mnemonic="S"
                  tooltip="TSQM (Main) Tooltip">
            </command>
            <command
                  commandId="com.tsqm.plugin.commands.sampleCommand2"
                  id="com.tsqm.plugin.menus.sampleCommand2"
                  mnemonic="S"
                  tooltip="TSQM Command">
            </command>
            <command
                  commandId="com.tsqm.plugin.commands.sampleCommand3"
                  id="com.tsqm.plugin.menus.sampleCommand3"
                  mnemonic="S"
                  tooltip="TSQM Command">
            </command>
            <command
                  commandId="com.tsqm.plugin.commands.sampleCommand4"
                  id="com.tsqm.plugin.menus.sampleCommand4"
                  mnemonic="S"
                  tooltip="TSQM Command">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.tsqm.plugin.toolbars.sampleToolbar">
            <command
                  commandId="com.tsqm.plugin.commands.sampleCommand"
                  icon="icons/scau.gif"
                  tooltip="TSQM Toolbar"
                  id="com.tsqm.plugin.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="TSQMAction Set"
            visible="true"
            id="com.tsqm.plugin.actionSet">
         <menu
               label="TSQM &amp;Menu"
               id="TSQMMenu">
            <separator
                  name="TSQMGroup">
            </separator>
         </menu>
         <action
               label="&amp;TSQM Action"
               icon="icons/sample.gif"
               class="com.tsqm.plugin.actions.TSQMAction"
               tooltip="TSQM Menu Tooltip"
               menubarPath="TSQMMenu/TSQMGroup"
               toolbarPath="sampleGroup"
               id="com.tsqm.plugin.actions.TSQMAction">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="testToc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            name="TSQM File Import"
            id="com.tsqm.plugin.importWizards.sampleCategory">
      </category>
      <wizard
            name="Import File for TSQM Benchmark"
            icon="icons/sample.gif"
            category="com.tsqm.plugin.importWizards.sampleCategory"
            class="com.tsqm.plugin.importWizards.ImportWizard"
            id="com.tsqm.plugin.importWizards.ImportWizard">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="TSQM Wizards"
            id="com.tsqm.plugin">
      </category>
      <wizard
            name="TSQM Multi-page Editor file"
            icon="icons/sample.gif"
            category="com.tsqm.plugin"
            class="com.tsqm.plugin.wizards.TSQMNewWizard"
            id="com.tsqm.plugin.wizards.TSQMNewWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="TSQM Release Engineering"
            icon="icons/releng_gears.gif"
            class="com.tsqm.plugin.perspectives.RelEngPerspective"
            id="com.tsqm.plugin.perspectives.RelEngPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="TSQM Benchmark Preferences"
            class="com.tsqm.plugin.preferences.TSQMPreferencePage"
            id="com.tsqm.plugin.preferences.TSQMPreferencePage">
      </page>
      <page
            class="com.tsqm.plugin.preferences.MetricsPreferencePage"
            id="com.tsqm.plugin.preferences.MetricsPreferencePage"
            name="TSQM Metrics Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.tsqm.plugin.preferences.PreferenceInitializer">
      </initializer>
      <initializer
            class="com.tsqm.plugin.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            name="TSQM Benchmark Page"
            nameFilter="*.*"
            class="com.tsqm.plugin.properties.TSQMPropertyPage"
            id="com.tsqm.plugin.properties.samplePropertyPage">
         <enabledWhen>
            <instanceof
                  value="org.eclipse.core.resources.IFile">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="com.tsqm.plugin.splashHandlers.InteractiveSplashHandler"
            id="com.tsqm.plugin.splashHandlers.interactive">
      </splashHandler>
      <splashHandlerProductBinding
            splashId="com.tsqm.plugin.splashHandlers.interactive"
            productId="org.eclipse.epp.package.jee.product">
      </splashHandlerProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            content="TSQM Benchmark Intro/sample.xml"
            configId="org.eclipse.ui.intro.universalConfig">
      </configExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
